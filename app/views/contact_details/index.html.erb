<h1>All contact details</h1>

<table>
  <thead>
  <tr>
    <th>Full name</th>
    <th>Attachment</th>

  </tr>
  </thead>

  <tbody>
  <% @contact_details.each do |contact_detail| %>
      <tr>
        <td><%= contact_detail.full_name %></td>

        <td><%= link_to "Download Resume", contact_detail.attachment_url %></td>

      </tr>
  <% end %>
  </tbody>
</table>

